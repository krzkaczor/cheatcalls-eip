{
  "name": "cheatcalls-tests",
  "version": "1.0.0",
  "private": "true",
  "engines": {
    "node": ">=22.5.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "check": "biome check .",
    "check:fix": "biome check --write --unsafe .",
    "verify": "concurrently --names \"LINT,TYPECHECK\" -c \"bgMagenta.bold,bgBlue.bold\" \"pnpm run lint\" \"pnpm run typecheck\"",
    "lint": "eslint src",
    "test": "node -r dotenv/config --import tsx --test \"**/*.test.ts\"",
    "test:only": "node -r dotenv/config --import tsx --test --test-reporter spec --test-only \"**/*.test.ts\"",
    "typecheck": "tsc --noEmit",
    "fix": "pnpm run check:fix && pnpm run verify"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@eslint/js": "^9.8.0",
    "@types/node": "^22.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.8.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "8.0.0-alpha.58"
  },
  "dependencies": {
    "@viem/anvil": "^0.0.10",
    "dotenv": "^16.4.5",
    "fetch-retry": "^6.0.0",
    "viem": "^2.18.6",
    "zod": "^3.23.8"
  }
}
